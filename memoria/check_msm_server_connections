#!/bin/bash

CONN_THRESHOLD=10
PNAME="node"

result=`ssh $MSMSERVER /bin/bash --login << EOF
  open_connections -s $PNAME 
EOF`

if [ $? -ne 0 ]; then
  beep s1s1s1l1s1s1s1
  exit 1
fi

cnt="${result##*:}"
if [ "$cnt" -ge "$CONN_THRESHOLD" ]; then
  beep l1l1s1s1s1l1l
  exit 1
fi

echo "s'all good man"

